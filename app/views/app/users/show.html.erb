<div class="header d-flex">
    <%= link_to image_tag('back_arrow.png', width: "50px", height: "50px", class: "mt-4", style: "position:absolute;"), root_path, data: { 'no-turbolink' => true }, class: "ml-4" %>
</div>
    <h2 class="text-center mx-auto mt-4 mb-1 display-3">Thanks for Signing Up!</h2>

